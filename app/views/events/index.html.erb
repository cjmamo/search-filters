<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Country</th>
    <th>Locality</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td><%= event.country %></td>
    <td><%= event.locality %></td>
    <td><%= event.start_date %></td>
    <td><%= event.end_date %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
